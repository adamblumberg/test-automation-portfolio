        {
          "name": "Get all boards",
          "event": [
            {
              "listen": "test",
              "script": {
                "exec": [
                  "pm.test('Status code is 200', () => {\r",
                  "    pm.response.to.have.status(200)\r",
                  "    })\r",
                  "\r",
                  "//create a response variable\r",
                  "const response = pm.response.json();\r",
                  "\r",
                  "//log the number of boards, then get their names\r",
                  "console.log(\"Number of objects:\", response.length);\r",
                  "response.forEach((board, index) => {\r",
                  "    console.log(`Board ${index + 1}: ${board.name}: ${board.id}`);\r",
                  "});\r",
                  "\r",
                  "const board = response.find(item => item.name === \"Board to delete\")\r",
                  "if (board && board.id) {\r",
                  "    pm.collectionVariables.set(\"boardId\", board.id);\r",
                  "    console.log (\"Board id found. Set to:\", board.id)\r",
                  "} else {\r",
                  "    console.log(\"Could not find: board to delete\")\r",
                  "}"
                ],
                "type": "text/javascript",
                "packages": {},
                "requests": {}
              }
            }
          ],
          "request": {
            "method": "GET",
            "header": [],
            "url": {
              "raw": "{{baseUrl}}/1/members/me/boards?key={{trelloKey}}&token={{trelloToken}}",
              "host": [
                "{{baseUrl}}"
              ],
              "path": [
                "1",
                "members",
                "me",
                "boards"
              ],
              "query": [
                {
                  "key": "key",
                  "value": "{{trelloKey}}"
                },
                {
                  "key": "token",
                  "value": "{{trelloToken}}"
                }
              ]
            }
          },
          "response": []
        },
        {
          "name": "Create board",
          "event": [
            {
              "listen": "test",
              "script": {
                "exec": [
                  "pm.test('Status code is 200', () => {\r",
                  "    pm.response.to.have.status(200)\r",
                  "    })\r",
                  "\r",
                  "let response = pm.response.json()\r",
                  "\r",
                  "let boardId = response.id;\r",
                  "pm.collectionVariables.set(\"boardId\", boardId);\r",
                  "\r",
                  "pm.test(\"Board is created\", () => {\r",
                  "    pm.expect(response.name).to.eql(\"Test Board to delete \" + pm.collectionVariables.get(\"boardNumber\"));\r",
                  "})\r",
                  "\r",
                  "pm.test(\"Board is OPEN\", () => {\r",
                  "    pm.expect(response.closed).to.eql(false);\r",
                  "})\r",
                  "\r",
                  "pm.test(\"Board is private\", () => {\r",
                  "    pm.expect(response.prefs.permissionLevel).to.eql(\"private\");\r",
                  "})\r",
                  "\r",
                  "pm.test(\"Calendar is disabled\", () => {\r",
                  "    pm.expect(response.prefs.switcherViews[2].enabled).to.eql(false);\r",
                  "})\r",
                  "\r",
                  "pm.test(\"Calendar is disabled find method\", () => {\r",
                  "    const calendarView = response.prefs.switcherViews.find(view => view.viewType === 'Calendar')\r",
                  "    pm.expect(calendarView.enabled).to.eql(false);\r",
                  "})"
                ],
                "type": "text/javascript",
                "packages": {},
                "requests": {}
              }
            },
            {
              "listen": "prerequest",
              "script": {
                "exec": [
                  "\r",
                  "let boardNumber = pm.collectionVariables.get('boardNumber')\r",
                  "if (isNaN(boardNumber)) {\r",
                  "    boardNumber = 0\r",
                  "}\r",
                  "boardNumber++;\r",
                  "pm.collectionVariables.set('boardNumber', boardNumber);"
                ],
                "type": "text/javascript",
                "packages": {},
                "requests": {}
              }
            }
